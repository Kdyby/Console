{
	"name": "kdyby/console",
	"type": "library",
	"description": "Symfony Console integration for Kdyby components",
	"keywords": ["nette", "kdyby", "console"],
	"homepage": "http://kdyby.org",
	"license": ["BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
	"authors": [
		{
			"name": "Filip Proch√°zka",
			"homepage": "http://filip-prochazka.com",
			"email": "filip@prochazka.su"
		}
	],
	"support": {
		"email": "filip@prochazka.su",
		"issues": "https://github.com/kdyby/console/issues"
	},
	"repositories": [
		{
			"type": "git",
			"url": "https://github.com/paveljurasek/events"
		}
	],
	"require": {
		"php": "^7.1",
		"nette/di": "^3.0",
		"tracy/tracy": "^2.6",
		"symfony/console": "~2.3 || ^3.0 || ^4.0"
	},
	"require-dev": {
		"nette/application": "^3.0",
		"nette/bootstrap": "^3.0",
		"nette/caching": "^3.0",
		"nette/http": "^3.0",
		"nette/tester": "^2.2",
		"kdyby/events": "^4.0",
		"symfony/event-dispatcher": "~2.3 || ^3.0 || ^4.0",
		"kdyby/coding-standard": "^1.0@dev",
		"phpstan/phpstan-shim": "^0.11"
	},
	"suggest": {
		"nette/http": "For faking the url of the request in cli"
	},
	"autoload": {
		"psr-4": {
			"Kdyby\\Console\\": "src/"
		},
		"classmap": [
			"src/CliPresenter.php",
			"src/exceptions.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"KdybyTests\\": "tests/KdybyTests/"
		},
		"classmap": [
			"tests/KdybyTests/"
		]
	},
	"extra": {
		"branch-alias": {
			"dev-master": "2.7-dev"
		}
	}
}
